{
  "name": "@creately/mungo",
  "version": "1.2.1",
  "description": "",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf coverage dist",
    "build": "tsc",
    "lint": "prettier --print-width 120 --single-quote --trailing-comma es5 --write \"src/**/*.ts\"",
    "test": "karma start karma.conf.js --single-run",
    "test:mongo": "cd .mongo-rest && PORT=3000 MONGO_URL=mongodb://localhost:27017/mungo node index.js",
    "test:watch": "karma start karma.conf.js",
    "test:cover": "npm run test && open coverage/index.html",
    "prepublish": "npm run clean && npm run lint && npm run build"
  },
  "keywords": [
    "mongo",
    "update",
    "documents"
  ],
  "author": "Muhammed Thanish <mnmtanish@gmail.com> (http://thanish.me/)",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^3.3.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^3.1.0",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-jasmine": "^2.0.1",
    "karma-webpack": "^3.0.0",
    "prettier": "^1.13.6",
    "rimraf": "^2.6.2",
    "ts-loader": "^5.3.0",
    "typescript": "^3.1.6",
    "webpack": "^4.12.2"
  },
  "dependencies": {
    "mingo": "^2.2.2"
  }
}
